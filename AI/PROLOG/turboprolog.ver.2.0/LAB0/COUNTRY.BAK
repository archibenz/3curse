DOMAINS
    city, country = symbol

PREDICATES
    situ(city, country)

CLAUSES
    situ(moscow, russia).
    situ(spb, russia).
    situ(berlin, germany).
    situ(munich, germany).
    situ(paris, france).
    situ(lyon, france).
    situ(tokyo, japan).
    situ(osaka, japan).
    situ(cairo, egypt).
    situ(alexandria, egypt).
    situ(sidney, australia).
    
    situ(X, auistraliya) :- situ(X, australia).
    situ(X, europe) :- situ(X, russia).
    situ(X, europe) :- situ(X, germany).
    situ(X, europe) :- situ(X, france).
    situ(X, asia)   :- situ(X, japan).
    situ(X, africa) :- situ(X, egypt).